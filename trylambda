#!/usr/bin/env python3

import lark

def main():
    with open("grammar.lark") as f:
        l = lark.Lark(f.read())

    for expr in ["x", "x y", "x y z", "x (y z)", "Î»x y. y x"]:
        print(expr)
        parse = l.parse(expr)
        print(parse.pretty())

if __name__ == '__main__':
    main()
