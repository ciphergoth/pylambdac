start: (directive ";")*
?directive: let | test
let: "let" var "=" apply
test: "test" apply "==" apply
?apply: subexpr | mlambda | apply subexpr
?subexpr: var | "(" apply ")"
mlambda: ("Î»" | "\\") var* "." apply
var: VARWORD
VARWORD: ("_"|LETTER|DIGIT)+

COMMENT: /#[^\n]*/
%ignore WS
%ignore COMMENT

%import common.LETTER
%import common.DIGIT
%import common.WS
