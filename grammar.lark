directives: (directive ";")*
?directive: let | reduce
let: "let" VARWORD "=" expr
reduce: "reduce" expr
?expr: subexpr | apply
apply: expr subexpr
?subexpr: var | mlambda | "(" expr ")"
mlambda: ("Î»" | "\\") varlist "." expr
varlist: VARWORD*
var: VARWORD
VARWORD: ("_"|LETTER|DIGIT)+

COMMENT: /#[^\n]*/
%ignore WS
%ignore COMMENT

%import common.LETTER
%import common.DIGIT
%import common.WS
