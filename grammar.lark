start: (directive ";")*
?directive: let | test
let: "let" var "=" apply
test: "test" apply "==" apply
?apply: subexpr  | apply subexpr
?subexpr: var | mlambda | "(" apply ")"
mlambda: ("Î»" | "\\") varlist "." apply
varlist: VARWORD*
var: VARWORD
VARWORD: ("_"|LETTER|DIGIT)+

COMMENT: /#[^\n]*/
%ignore WS
%ignore COMMENT

%import common.LETTER
%import common.DIGIT
%import common.WS
